{% extends 'application.njk' %}

{% block layout %}
  {% include 'partials/page-heading.njk' %}
  {# include 'partials/breadcrumbs.njk' #}
  <div class="dsRow">
    <div class="dsPage dsCol-12-xs dsCol-9-md dsOrder-2-xs dsOrder-1-md">
      <div class="dsPage__body">

        <section>{# Inheritance #}
          {% block inheritance %}
            <ul>
              <li>HTMLElement
                <ul>
                  <li><i>{{page.title}}</i></li>
                </ul>
              </li>
            </ul>
          {% endblock %}
        </section>

        <section>{# Properties #}
          <h2 id="properties">Properties</h2>
          {% block properties %}
            <p>
              <i>None specified; inherits properties from its parent.</i>
            </p>
          {% endblock %}
        </section>

        <section>{# Methods #}
          <h2 id="methods">Methods</h2>
          {% block methods %}
            <p>
              <i>None specified; inherits methods from its parent.</i>
            </p>
          {% endblock %}
        </section>

        {% block content %}
          {# additional page content goes here #}
        {% endblock %}
      </div>
      {% include 'partials/page-footer.njk' %}
    </div>
    <div class="dsSidebar dsCol dsCol-12-xs dsCol-3-md dsOrder-1-xs dsOrder-2-md">
      <h6 class="dsLabel">On this page:</h6>
      <aside class="article-toc" role="navigation">
        <a href="#properties">Properties</a>
        <a href="#methods">Methods</a>
        {{ page.raw | toc | safe }}
        <hr />
        <a class="top-link" href="#top">Back to Top</a>
      </aside>
    </div>
  </div>
{% endblock %}
